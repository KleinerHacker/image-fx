<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pcsoft.tools.image_fx.ui.splash.controllers.SplashController">
   <children>
      <ImageView fitHeight="723.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
         <image>
             <Image url="images/imagefx_bg.png" />
         </image>
      </ImageView>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="723.0" prefWidth="1024.0">
         <bottom>
            <VBox BorderPane.alignment="CENTER">
               <children>
                  <ProgressBar fx:id="pbCompleteProgress" maxWidth="1.7976931348623157E308" progress="0.0">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </ProgressBar>
                  <ProgressBar fx:id="pbPartProgress" maxWidth="1.7976931348623157E308" progress="0.0">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </ProgressBar>
                  <Label fx:id="lblAction" style="-fx-text-fill: #FFFFFF;" text="Loading...">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
         </bottom>
      </BorderPane>
   </children>
</StackPane>
